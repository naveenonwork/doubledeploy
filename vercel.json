{
    "version": 2,
    "outputDirectory": ".",
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["./frontend/dist/**"]
        }
      }
    ],
    "routes": [{ "src": "/(.*)", "dest": "/" }],
    "functions": {
      "static/avatar/": {
        "memory": 3008,
        "maxDuration": 200
      },
      "static/garment": {
        "memory": 3008,
        "maxDuration": 300
      }
    }
  }